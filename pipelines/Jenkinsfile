node {
    String branchName = 'jenkins_setup'
    String repoUrl = 'https://github.com/HighLandner/docker_agent'
    
    // options {
    //     buildDiscarder(logRotator(numToKeepStr: '5'))
    // }

    stage('Clone') {
        sh 'mkdir -p build'
        dir('build') {
            git branch: branchName, url: repoUrl
        }
    }
    post {
        always {
            cleanWs deleteDirs: true
        }
    }
}